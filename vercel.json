{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/bot/action(.*)",
      "dest": "/api/action.js"
    },
    {
      "src": "/api/bot/status(.*)",
      "dest": "/api/status.js"
    },
    {
      "src": "/api/bot/logs/stream(.*)",
      "dest": "/api/logs.js"
    },
    {
      "src": "/api/bot/logs(.*)",
      "dest": "/api/logs.js"
    },
    {
      "src": "/api/settings(.*)",
      "dest": "/api/settings.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
