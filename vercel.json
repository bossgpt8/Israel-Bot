{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/bot/status",
      "dest": "/api/bot/status.js"
    },
    {
      "src": "/api/bot/action",
      "dest": "/api/bot/action.js"
    },
    {
      "src": "/api/bot/logs",
      "dest": "/api/bot/logs/index.js"
    },
    {
      "src": "/api/settings",
      "dest": "/api/settings/index.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "check": true
    }
  ]
}
